<?xml version="1.0" encoding="UTF-8" ?>
<phpdocumentor configVersion="3" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="https://www.phpdoc.org" xsi:schemaLocation="https://www.phpdoc.org https://www.phpdoc.org/schema/3.0.xsd">
  <title>Let's Travel - Developer Documentation</title>
  <version number="1.0.0">1.0</version>
  <paths>
    <output>docs</output>
    <cache>docs/cache</cache>
  </paths>
  <settings>
    <default-package-name>TravelAgency</default-package-name>
  </settings>
  <template name="default">
    <transformations>
      <transformation>
        <type>clean</type>
        <artifact>docs/html</artifact>
      </transformation>
    </transformations>
  </template>
</phpdocumentor>

